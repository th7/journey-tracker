<div class="container">
	<h1>Photos</h1>

	<% if @trip.photos.any? %>
		<p>Photo List:</p>
		<p>Trip ID in session: <%= session[:current_trip] %></p>
		<ul>
			<% @trip.photos.each do |photo| %>
				<li><%= link_to photo.url, trip_photo_path(photo.trip, photo),:target => "_blank" %> - Trip: <%= photo.trip_id %></li>
			<% end %>
		</ul>
	<% end %>
</div>

